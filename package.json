    "e2e-cli-test": "cypress run",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@eslint/js": "^9.0.0",
    "babel-jest": "^29.7.0",
    "cypress": "^13.7.3",
    "eslint": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-jest": "^28.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "live-server": "^1.2.2",
    "prettier": "3.2.5",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "lint-staged": {
    "*.js": [
